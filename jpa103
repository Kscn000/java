import java.util.Scanner;
public class jpa103 {

    public static void main(String[] args) {
        System.out.println("Please input:");
		Scanner sc = new Scanner(System.in);

        int a,b,c;

        a=sc.nextInt();
        b=sc.nextInt();
        c=sc.nextInt();

        System.out.printf("Average: %4.2f",((double)(a+b+c)/3));
        
    
    }
}
